{
  "java.project.sourcePaths": ["src"],
  "java.project.outputPath": "bin",
  "java.project.referencedLibraries": ["lib/**/*.jar"],
  // Add a runtime entry pointing to your JDK 21 installation.
  // Replace the path below with the actual installation path on your machine.
  // After installing JDK 21, restart VS Code.
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-21",
      "path": "C:/Program Files/Java/jdk-21",
      "sourcePath": "",
      "default": true
    }
  ]
}
